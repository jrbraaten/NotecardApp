<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jrbra.notecardapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HelpPageActivity"
            android:label="@string/title_activity_help_page" >
        </activity>
        <activity
            android:name=".CreatePageActivity"
            android:label="@string/title_activity_create_page" >
        </activity>
        <activity
            android:name=".AboutPageActivity"
            android:label="@string/title_activity_about_page" >
        </activity>
        <activity
            android:name=".LearnPageActivity"
            android:label="@string/title_activity_learn_page" >
        </activity>
    </application>

</manifest>
